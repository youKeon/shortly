[mysqld]
# Server identification
server-id = 2

# Relay logging for replication
relay-log = mysql-relay-bin
log-bin = mysql-bin
binlog_format = ROW

# Read-only mode
read_only = 1
super_read_only = 1

# Character set
character-set-server = utf8mb4
collation-server = utf8mb4_unicode_ci

# Timezone
default-time-zone = '+09:00'
